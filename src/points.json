{
    "base" : "https://api.spotify.com",
    "access_token" : {
        "url": "https://accounts.spotify.com/api/token",
        "headers" : {
            "Content-Type" : "application/x-www-form-urlencoded"
        },
        "body" : "grant_type=client_credentials&client_id=$CLIENT_ID&client_secret=$CLIENT_SECRET"
    }
}